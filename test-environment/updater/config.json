{
    "FilesystemPath": "downloads",
    "FilesPermission": "770",
    "DirectoriesPermission": "770",
    "EtcdKeyPrefix": "/upload/",
    "EtcdEndpoints": ["127.0.0.1:32381"],
    "CaCertPath": "../etcd-server/certs/ca.pem",
    "UserAuth": {
        "CertPath": "../etcd-server/certs/root.pem",
        "KeyPath": "../etcd-server/certs/root.key"
    },
    "ConnectionTimeout": "30s",
    "RequestTimeout": "30s",
    "RequestRetries": 5,
    "NotificationCommand": ["echo", "updated!"],
    "NotificationCommandRetries": 5
}